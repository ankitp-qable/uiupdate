                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{dashboardVM.dashboardModel.txtTheHealthiest}"
                        style="@style/txtIntersemibold10_1" tools:text="@string/msg_the_25_healthie"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup257"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.54"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{dashboardVM.dashboardModel.txtLanguage}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_jun_10_20212"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_500_radius_1"
                            android:id="@+id/viewEllipse1"
                            android:layout_height="@dimen/_2pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_2pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_110pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_weight="0.44"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{dashboardVM.dashboardModel.txtTime}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_5min_read2"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup274"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameGroup277"
                android:layout_height="@dimen/_50pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageBottonBarbackg"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_width="@dimen/_375pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle12"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle12"/>
                <LinearLayout android:id="@+id/linearGroup40"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup36"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageHomeIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_homeicon_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_homeicon_1"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtHome"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{dashboardVM.dashboardModel.txtHome}"
                            style="@style/txtIntermedium8_1" tools:text="@string/lbl_home"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup37"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageMessagesIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_messagesicon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_messagesicon"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtMessages"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{dashboardVM.dashboardModel.txtMessages}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_messages"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup38"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageAppointmentIco"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_appointmentico_2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_appointmentico_2"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAppointment"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{dashboardVM.dashboardModel.txtAppointment}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_appointment"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup39"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageProfileicon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_profileicon_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_profileicon_1"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtProfile"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{dashboardVM.dashboardModel.txtProfile}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_profile"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
