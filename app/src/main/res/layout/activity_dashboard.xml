                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginStart="@dimen/_118pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notificationic"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notificationic"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup269"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <EditText
                android:background="@drawable/rectangle_bg_gray_50_border_bluegray_50_radius_8"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_iconly_lightoutline_search_3"
                android:ellipsize="end"
                android:hint="@string/msg_search_doctor"
                android:id="@+id/etSearch" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingStart="@dimen/_17pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@={dashboardVM.dashboardModel.etSearchValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGroup262"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageDoctorIcon"
                    android:layout_height="@dimen/_71pxh"
                    android:layout_width="@dimen/_71pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_doctoricon"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_doctoricon"/>
                <ImageView android:id="@+id/imageMedicineIcon"
                    android:layout_height="@dimen/_71pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_width="@dimen/_71pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_medicineicon"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_medicineicon"/>
                <ImageView android:id="@+id/imageAmbulanceIcon"
                    android:layout_height="@dimen/_71pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_width="@dimen/_71pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_ambulanceicon"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ambulanceicon"/>
                <ImageView android:id="@+id/imageArticalsIcon"
                    android:layout_height="@dimen/_71pxh"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_width="@dimen/_71pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_articalsicon"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_articalsicon"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGroup270"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_50cornerRadius">
                <TextView android:id="@+id/txtEarlyprotectio"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:text="@{dashboardVM.dashboardModel.txtEarlyprotectio}"
                    style="@style/txtIntersemibold20" tools:text="@string/msg_early_protectio"/>
                <TextView android:id="@+id/txtLearnmorebutt"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingStart="@dimen/_21pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:text="@{dashboardVM.dashboardModel.txtLearnmorebutt}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_learn_more"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearTop"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_39pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTopDoctor"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardVM.dashboardModel.txtTopDoctor}"
                    style="@style/txtIntersemibold16" tools:text="@string/lbl_top_doctor"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSeeall"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_210pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardVM.dashboardModel.txtSeeall}"
                    style="@style/txtInterregular12_1" tools:text="@string/lbl_see_all"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerDoctor"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_14pxh}" tools:itemCount="3" tools:listitem="@layout/row_doctor"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearHealt"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHealtarticle"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.71"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{dashboardVM.dashboardModel.txtHealtarticle}"
                    style="@style/txtIntersemibold16" tools:text="@string/lbl_healt_article"/>
