<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settignsVM" type="com.ankitsapplication.app.modules.settigns.data.viewmodel.SettignsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearGroup200"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle">
        <LinearLayout android:id="@+id/linearGroup192"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <ImageView android:id="@+id/imageMoreicon"
                android:layout_gravity="end"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_moreicon"
                tools:ignore="ContentDescription" tools:src="@drawable/img_moreicon"/>
            <ImageView android:id="@+id/imageProfilephotog"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_80pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="@dimen/_80pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_profilephotog"
                tools:ignore="ContentDescription" tools:src="@drawable/img_profilephotog"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAmeliaRenata"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{settignsVM.settignsModel.txtAmeliaRenata}"
                style="@style/txtIntersemibold18_1" tools:text="@string/lbl_amelia_renata"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerGroup198"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_61pxh}" tools:itemCount="3" tools:listitem="@layout/row_group198"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearOptions"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_43pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_4">
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerGroup199"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_55pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_27pxh}" tools:itemCount="5" tools:listitem="@layout/row_group199"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup194"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameGroup197"
                android:layout_height="@dimen/_50pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle12"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_width="@dimen/_375pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle12"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle12"/>
                <LinearLayout android:id="@+id/linearGroup28"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup24"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageHomeIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_homeicon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_homeicon"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtHome"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{settignsVM.settignsModel.txtHome}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_home"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup25"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageMessagesicon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_messagesicon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_messagesicon"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtMessages"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{settignsVM.settignsModel.txtMessages}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_messages"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup26"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageAppointmentico"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_appointmentico_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_appointmentico_1"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAppointment"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{settignsVM.settignsModel.txtAppointment}"
                            style="@style/txtIntermedium8" tools:text="@string/lbl_appointment"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup27"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageProfileIcon"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_profileicon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_profileicon"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtProfile"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{settignsVM.settignsModel.txtProfile}"
                            style="@style/txtIntermedium8_1" tools:text="@string/lbl_profile"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
