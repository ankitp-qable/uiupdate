<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drugDetailsVM" type="com.ankitsapplication.app.modules.drugdetails.data.viewmodel.DrugDetailsVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearDrugDetails"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup62"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="?attr/actionBarSize"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearHeadergroup"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIconlyLightAr"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconlylightar_1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconlylightar_1"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDrugsdetail"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_96pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.66"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drugDetailsVM.drugDetailsModel.txtDrugsdetail}"
                        style="@style/txtIntersemibold16" tools:text="@string/lbl_drugs_detail"/>
                    <ImageView android:id="@+id/imageIconlyLightBu"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_96pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconlylightbu"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconlylightbu"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageDrugimage"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_147pxh"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_64pxv"
                android:layout_width="@dimen/_147pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_drugimage}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_drugimage"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearGroup59"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_64pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearGroup58"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearDetailgroup"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_weight="0.82"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOBHCombi"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{drugDetailsVM.drugDetailsModel.txtOBHCombi}"
                            style="@style/txtIntersemibold20" tools:text="@string/lbl_obh_combi"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMeasurement"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{drugDetailsVM.drugDetailsModel.txtMeasurement}"
                            style="@style/txtIntersemibold16_1" tools:text="@string/lbl_75ml"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearGroup79"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageIconlyBoldSta"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconlyboldsta"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconlyboldsta"/>
                            <ImageView
                                android:id="@+id/imageIconlyBoldSta1"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconlyboldsta"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconlyboldsta"/>
                            <ImageView
                                android:id="@+id/imageIconlyBoldSta2"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconlyboldsta"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconlyboldsta"/>
                            <ImageView
                                android:id="@+id/imageIconlyBoldSta3"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_14pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconlyboldsta"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconlyboldsta"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtRatting"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_weight="0.29"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{drugDetailsVM.drugDetailsModel.txtRatting}"
                                style="@style/txtIntersemibold14_1" tools:text="@string/lbl_4_0"/>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageIconlyBoldHea"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_27pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_199pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconlyboldhea"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconlyboldhea"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearQuantitygroup"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup64"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.63"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageComponent3"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_width="@dimen/_32pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_component3_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_component3_1"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtQuantitycount"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.16"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{drugDetailsVM.drugDetailsModel.txtQuantitycount}"
                            style="@style/txtIntersemibold24" tools:text="@string/lbl_1"/>
                        <ImageView android:id="@+id/imageComponent2"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_marginStart="@dimen/_28pxh"
                            android:layout_width="@dimen/_32pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_component2_1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_component2_1"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_134pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.37"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{drugDetailsVM.drugDetailsModel.txtPrice}"
                        style="@style/txtIntersemibold26" tools:text="@string/lbl_9_99"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{drugDetailsVM.drugDetailsModel.txtDescription}"
                style="@style/txtIntersemibold16" tools:text="@string/lbl_description"/>
            <TextView android:id="@+id/txtDescription1"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@{drugDetailsVM.drugDetailsModel.txtDescription1}"
                style="@style/txtInterregular12" tools:text="@string/msg_obh_combi_is_a"/>
            <LinearLayout android:id="@+id/linearGroup60"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_77pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCarticon"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_carticon"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_carticon"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnBuyNow"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_buy_now"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_buy_now"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
