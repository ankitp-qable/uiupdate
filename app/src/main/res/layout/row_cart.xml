<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cartRowModel" type="com.ankitsapplication.app.modules.cart.data.model.CartRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_1">
        <ImageView android:id="@+id/imageDrugthumbnail"
            android:layout_height="@dimen/_72pxh"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="@dimen/_72pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_drugthumbnail}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_drugthumbnail"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup263"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginStart="@dimen/_42pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_weight="0.45" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtOBHCombi"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cartRowModel.txtOBHCombi}"
                style="@style/txtIntersemibold18" tools:text="@string/lbl_obh_combi"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMeasurement"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cartRowModel.txtMeasurement}"
                style="@style/txtIntermedium12" tools:text="@string/lbl_75ml"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearQualtitygroup"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageComponent5"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_component3"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_component3"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCounter"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{cartRowModel.txtCounter}"
                    style="@style/txtIntersemibold16" tools:text="@string/lbl_1"/>
                <ImageView android:id="@+id/imageComponent2"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_component2"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_component2"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearGroup49"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_weight="0.23" android:layout_width="0dp" android:orientation="vertical">
            <ImageView android:id="@+id/imageIconlyLightDe"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_iconlylightde"
                tools:ignore="ContentDescription" tools:src="@drawable/img_iconlylightde"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{cartRowModel.txtPrice}"
                style="@style/txtIntersemibold18" tools:text="@string/lbl_9_99"/>
        </LinearLayout>
    </LinearLayout>
</layout>
